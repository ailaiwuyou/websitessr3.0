{
    "root": true,
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "no-console": "off",
        "func-names": "off",
        "vue/comment-directive": "off"
    }
}
